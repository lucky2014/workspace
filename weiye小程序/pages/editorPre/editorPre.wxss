.footAll{
    position:fixed;
    bottom:20px;
    height:48px;
    width:258px;
    left:50%;
    margin-left:-129px;
    z-index:59;
}
.footAll>view{
    height:100%;
    width:128px;
    float:left;
    background-color:#FF5900;
    box-shadow:0 1px 2px #000;
    font-size:18px;
    color:#fff;
}
.footAll .footLeft{
    border-radius:24px 0 0 24px;
}
.footAll .footRight{
    border-radius:0 24px 24px 0;
}
.footAll image{
    width:20px;
    margin-top:15px;
    margin-left:30px;
    float:left;
}
.footAll view view{
    float:left;
    margin-top:13px;
    margin-left:5px;
}