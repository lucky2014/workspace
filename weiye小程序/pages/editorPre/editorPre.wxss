page{background-color:#fff;}
.wrapper{padding: 0 16px;}
.title{padding-top: 1rem;margin-bottom: 0.2rem;font-weight: bold;letter-spacing: 1px;text-shadow: 1px 1px 0 hsla(0,0%,10%,.1);}
.desc{
	color: #1b2027;
    display: block;
    text-align: left;
    font-size: 14px;
}
.fourTap{
	display:none;
}
.addBox{
    margin:16px 0;
}
.mainImg{
	width :100%;
}
.link image{
	width: 14px;
    margin-top: 5px;
    float: left;
    height: 14px;
    background-color: transparent;
    display: inline-block;
}
.link{
	display: block;
    font-size: 14px;
    line-height: 24px;
    color: #1993e6;
}
.fontBox{
	margin:16px 0;
}
view{
    word-break:break-all;
}
.footer{
    padding:23px 0 5px 0;
    position:fixed;
    bottom:0;
    left:0;
    right:0;
    background-color:rgba(0,0,0,.9);
    height:104px;
    overflow-x:scroll;
    z-index:99;
}
.footer view view{
    display:inline-block;
    width:60px;
    height:90px;
    margin-left:15px;
}
.footer view view.choose{
    border:2px solid #1993e6;
    height:99px;
    width:66px;
    margin-top:0px;
}
.footer image{
    width:100%;
    height:100%;
}
.finish{
    position:fixed;
    bottom:140px;
    right:10px;
    height:32px;
    width:85px;
    border-radius:16px;
    text-align:center;
    line-height:32px;
    background:#FF5900;
    color:#fff;
    font-size:14px;
    z-index:99;
}
.tempate{
    position:fixed;
    bottom:140px;
    right:15px;
    height:60px;
    width:60px;
}
